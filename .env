# Application environment variables
# Set log level to DEBUG to see detailed flow logs
LOG_LEVEL=DEBUG

# Optional toggles for API logging (requests and responses)
# When true, logs full HTTP bodies for external APIs (may contain sensitive data)
LOG_HTTP_BODY=false
# When true, logs full LLM prompts and responses preview (may contain user content)
LOG_LLM_PROMPTS=true

# Optional: Alpha Vantage API key
# ALPHA_VANTAGE_API_KEY=your_key_here
# Optional: Mistral API key
# MISTRAL_API_KEY=your_key_here
